<Window x:Class="WpfApp1.AfterLogin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        xmlns:windowsFrom="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"
        xmlns:activeXControl="clr-namespace:AxWMPLib;assembly=AxInterop.WMPLib"
        Title="AfterLogin" Height="450" Width="800">
    <Grid Background="white">
        <DockPanel >
            <Button Content="Dock=Bottom" DockPanel.Dock="Bottom" Margin="78,0,-0.4,0"/>
            <StackPanel>
                <Button Content="Browse" Margin="0,0,0,-19.2" Width="78" Click="Button_Click_1"></Button>
                <ListBox x:Name="list" Margin="0,19,0,0" Height="382" SelectionChanged="ListBox_SelectionChanged">
                </ListBox>
            </StackPanel>
            

            <WindowsFormsHost x:Name="winsFormHost">
                <activeXControl:AxWindowsMediaPlayer x:Name="media">
                </activeXControl:AxWindowsMediaPlayer>
            </WindowsFormsHost>
        </DockPanel>
        
    </Grid>

</Window>
